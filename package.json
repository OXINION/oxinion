{
  "name": "oxinion",
  "version": "1.4.2",
  "description": "A modern SDK for location-based services",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "keywords": [
    "sdk",
    "location",
    "geofencing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^7.2.0",
    "turbo": "^2.5.0"
  },
  "packageManager": "pnpm@9.12.3",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
