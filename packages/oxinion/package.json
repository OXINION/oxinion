{
  "name": "oxinion",
  "version": "1.4.1",
  "description": "A modern SDK for location-based services",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@oxinion/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
