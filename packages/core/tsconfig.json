{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": "./"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  